{
  "extends":"airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules":{
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "no-unused-vars":0,
    "react/destructuring-assignment":0,
    "react/prop-types":0,
    "no-sequences":0
  },
  "parser":"babel-eslint"
}
